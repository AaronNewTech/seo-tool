{
    "rewrites": [
      {
        "source": "/api/:match*",
        "destination": "/api/:match*"
      }
    ],
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"
        }
      }
    ],
    "devCommand": "cd frontend && npm start"
  }
  