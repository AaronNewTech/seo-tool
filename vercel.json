{
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/pages/**",
        "use": "@vercel/next"
      }
    ],
    "rewrites": [
      {
        "source": "/api/:match*",
        "destination": "/api/:match*"
      }
    ],
    "buildCommand": "cd frontend && npm run build",
    "devCommand": "cd frontend && npm run dev"
  }
  